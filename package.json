    "@fullcalendar/timegrid": "^5.5.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "ajax": "0.0.4",
    "ant-design-pro": "^2.3.2",
    "antd": "^4.11.2",
    "antd-img-crop": "^3.9.0",
    "bizcharts": "^3.5.9",
    "bizcharts-plugin-slider": "^2.1.1",
    "classnames": "^2.2.6",
    "crypto-js": "^4.0.0",
    "dateformat": "^3.0.3",
