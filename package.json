    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "match-sorter": "^4.2.1",
    "moment": "^2.25.3",
    "numeral": "^2.0.6",
    "nzh": "^1.0.4",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.0",
    "react": "^16.8.6",
    "react-color": "^2.18.1",
    "react-data-export": "^0.6.0",
    "react-dom": "^16.8.6",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.0.4",
    "react-image-crop": "^8.6.6",
    "react-images-upload": "^1.2.8",
    "react-router": "^4.3.1",
    "react-to-print": "^2.9.0",
    "read-excel-file": "^5.0.0",
    "reqwest": "^2.0.5",
    "umi": "^3.3.9",
